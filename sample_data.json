{
  "type": "page",
  "title": "Ultimate Productivity Hub",
  "children": [
    {
      "type": "heading_1",
      "text": "Welcome to Your Productivity Hub"
    },
    {
      "type": "paragraph",
      "text": "Everything you need to stay organized, from grocery lists to project management, all in one place."
    },
    {
      "type": "divider"
    },
    {
      "type": "heading_2",
      "text": "Grocery List"
    },
    {
      "type": "database",
      "title": "Weekly Grocery Planning",
      "schema": {
        "Name": {
          "type": "title"
        },
        "Description": {
          "type": "rich_text"
        },
        "In stock": {
          "type": "checkbox"
        },
        "Food group": {
          "type": "select",
          "options": [
            {
              "name": "ü•¶Vegetable",
              "color": "green"
            },
            {
              "name": "üçéFruit",
              "color": "red"
            },
            {
              "name": "üí™Protein",
              "color": "yellow"
            }
          ]
        },
        "Price": {
          "type": "number",
          "format": "dollar"
        },
        "Last ordered": {
          "type": "date"
        },
        "Store availability": {
          "type": "multi_select",
          "options": [
            {
              "name": "Duc Loi Market",
              "color": "blue"
            },
            {
              "name": "Rainbow Grocery",
              "color": "gray"
            },
            {
              "name": "Nijiya Market",
              "color": "purple"
            },
            {
              "name": "Gus's Community Market",
              "color": "yellow"
            }
          ]
        },
        "+1": {
          "type": "people"
        },
        "Photo": {
          "type": "files"
        }
      }
    },
    {
      "type": "divider"
    },
    {
      "type": "heading_2",
      "text": "Tasks & Projects"
    },
    {
      "type": "toggle",
      "title": "Current Projects",
      "children": [
        {
          "type": "to_do_list",
          "items": [
            {"text": "Project 1 - Due Next Week", "checked": false},
            {"text": "Project 2 - In Progress", "checked": true}
          ]
        }
      ]
    },
    {
      "type": "toggle",
      "title": "General Tasks",
      "children": [
        {
          "type": "bulleted_list",
          "items": ["Task 1", "Task 2", "Task 3"]
        },
        {
          "type": "numbered_list",
          "items": ["Step 1", "Step 2", "Step 3"]
        }
      ]
    },
    {
      "type": "divider"
    },
    {
      "type": "heading_2",
      "text": "Meeting Notes"
    },
    {
      "type": "page",
      "title": "Notes Archive",
      "children": [
        {
          "type": "paragraph",
          "text": "A collection of notes from past meetings."
        }
      ]
    }
  ]
}

