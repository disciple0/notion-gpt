{
  "type": "page",
  "title": "Project Management Dashboard",
  "children": [
    {
      "type": "heading_1",
      "text": "Project Timelines"
    },
    {
      "type": "database",
      "title": "Project Timeline Tracker",
      "schema": {
        "Project Name": {
          "type": "title"
        },
        "Start Date": {
          "type": "date"
        },
        "End Date": {
          "type": "date"
        },
        "Team":{
          "type": "select",
          "options": [
            {
              "name": "Engineering",
              "color": "blue"
            },
            {
              "name": "Sales",
              "color": "orange"
            },
            {
              "name": "Marketing",
              "color": "green"
            }
          ]
        },
        "Status": {
          "type": "select",
          "options": [
            {
              "name": "Planning",
              "color": "blue"
            },
            {
              "name": "In Progress",
              "color": "yellow"
            },
            {
              "name": "Completed",
              "color": "green"
            }
          ]
        },
        "Details": {
          "type": "rich_text"
        },
        "Progress": {
          "type": "number",
          "format": "percent"
        }
      }
    },
    {
      "type": "heading_1",
      "text": "Task Management"
    },
    {
      "type": "database",
      "title": "Task Tracker",
      "schema": {
        "Task": {
          "type": "title"
        },
        "Assigned To": {
          "type": "people"
        },
        "Due Date": {
          "type": "date"
        },
        "Status": {
          "type": "select",
          "options": [
            {
              "name": "Not Started",
              "color": "red"
            },
            {
              "name": "In Progress",
              "color": "yellow"
            },
            {
              "name": "Completed",
              "color": "green"
            }
          ]
        },
        "Priority": {
          "type": "select",
          "options": [
            {
              "name": "High",
              "color": "red"
            },
            {
              "name": "Medium",
              "color": "yellow"
            },
            {
              "name": "Low",
              "color": "green"
            }
          ]
        }
      }
    },
    {
      "type": "heading_1",
      "text": "Bug Tracker"
    },
    {
      "type": "database",
      "title": "Bug Tracker",
      "schema": {
        "Bug Name": {
          "type": "title"
        },
        "Bud Description": {
          "type": "rich_text"
        },
        "Severity": {
          "type": "select",
          "options": [
            {
              "name": "Critical",
              "color": "red"
            },
            {
              "name": "Major",
              "color": "orange"
            },
            {
              "name": "Minor",
              "color": "yellow"
            },
            {
              "name": "Trivial",
              "color": "green"
            }
          ]
        },
        "Status": {
          "type": "select",
          "options": [
            {
              "name": "Open",
              "color": "red"
            },
            {
              "name": "In Review",
              "color": "yellow"
            },
            {
              "name": "Resolved",
              "color": "green"
            }
          ]
        },
        "Assigned To": {
          "type": "people"
        }
      }
    },
    {
      "type": "heading_1",
      "text": "Engineering Documentation"
    },
    {
